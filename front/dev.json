[
{
  "name"      : "DEV_SN_FRONT",
  "script"    : "./app.js",
  "args"      : "['--dev']",
  "ignoreWatch" : ["[\\/\\\\]\\./", "node_modules"],
  "instances" : 4,
  "exec_mode"  : "cluster_mode",
  "max_memory_restart": "256M",
  "error_file": "logs/dev_error.log",
  "out_file": "logs/dev_output.log",
  "merge_logs": true,
  "env" : {
    "NODE_ENV": "development",
    "PORT": "3500"
  }
}
]
